{
    "name": "brnbio/laravel-extended",
    "description": "Missing features for Laravel framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank Heider",
            "email": "heider@brnb.io"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Brnbio\\LaravelExtended\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brnbio\\LaravelExtended\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brnbio\\LaravelExtended\\ExtendedServiceProvider"
            ]
        }
    }
}
